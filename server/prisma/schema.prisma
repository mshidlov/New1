datasource postgres {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime @default(now())
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     String[]
  updatedAt DateTime @updatedAt
  username  String   @unique
}

model Bla {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  jbjkbkj   String?
  list      List?    @relation(fields: [listId], references: [id])
  listId    String?
  updatedAt DateTime @updatedAt
}

model List {
  blas      Bla[]
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}